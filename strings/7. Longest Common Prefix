14. Longest Common Prefix


class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
        int len = strs.size();
        
        if(len == 0)
            return "";
        
        sort(strs.begin(),strs.end());
        string prefix;
        string first = strs[0];
        string last = strs[len -1];
        
        for(int i = 0; i < first.size(); i++){
            if(first[i] ==last[i]){
                prefix = prefix + first[i];
            }
            else
                break;
            
            
        }
        return prefix;
        
    }
};
