class Solution {
public:
    int dominantIndex(vector<int>& nums) {
        int maxi = *max_element(nums.begin(), nums.end());
        int max_idx = 0;
        
        for(int i =0; i<nums.size(); i++){
            if(nums[i] == maxi){
                max_idx = i;
            }
        }
        
        for(int i =0; i< nums.size(); i++){
            if(2* nums[i] > maxi && i != max_idx){
                return -1;
            }
        }
        return max_idx;
        
        
    }
};
