PROBLEM 167
1. APPROACH 1 TWO POINTER SOLUTION

class Solution {
public:
    vector<int> twoSum(vector<int>& numbers, int target) {
        int first = 0;
        int last = numbers.size() - 1;
        while(first< last){
            int sum = numbers[first] + numbers[last];
            if (sum == target){
                return {first+1, last+1};
            }
            else if(sum < target){
                ++first;
            }
            else {
                -- last;
            }
        }
        return {-1,-1};
        
        
    }
};
